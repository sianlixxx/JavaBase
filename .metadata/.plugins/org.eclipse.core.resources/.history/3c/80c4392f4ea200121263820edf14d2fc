package com.miracle;

public class Mythread1 implements Runnable
{

	/**
	 * @param args
	 */
	public static void main(String[] args)
	{
		// TODO Auto-generated method stub

	}

	@Override
	public void run()
	{
		for (int i = 0; i < 10; i++)
		{
			if(this.ticket > 0)
			{
				System.out.println(this.name + "卖票" + (--this.ticket));
			}
			
		}
		
	}

}
